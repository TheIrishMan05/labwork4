{"ast":null,"code":"import { useAuthStore } from \"@/auth\";\nimport PasswordComponent from \"@/components/start_page_components/passwordInput.vue\";\nexport default {\n  name: 'LoginComponent',\n  components: {\n    PasswordComponent\n  },\n  props: {\n    error: String\n  },\n  data() {\n    return {\n      username: '',\n      password: ''\n    };\n  },\n  methods: {\n    async login() {\n      const authStore = useAuthStore();\n      if (!this.username || !this.password) {\n        this.$emit('setError', \"Не все поля заполнены.\" + \" Пожалуйста, заполните пропущенные поля и попробуйте ещё раз.\");\n      } else {\n        return authStore.login({\n          username: this.username,\n          password: this.password\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["useAuthStore","PasswordComponent","name","components","props","error","String","data","username","password","methods","login","authStore","$emit"],"sources":["C:\\study\\labs\\web\\weblab4\\front-vue\\src\\components\\start_page_components\\loginForm.vue"],"sourcesContent":["<template>\r\n  <div class=\"login-form\">\r\n    <h2>Регистрация в приложении</h2>\r\n    <form @submit.prevent=\"login\">\r\n      <div class=\"form-group\">\r\n        <label for=\"username\">Имя пользователя:</label>\r\n        <input\r\n            id=\"username\"\r\n            v-model=\"username\"\r\n            required\r\n            type=\"text\"\r\n        />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <PasswordComponent\r\n            id=\"password\"\r\n            v-model=\"password\"\r\n            label=\"Пароль:\"\r\n            placeholder=\"Введите пароль\"\r\n        />\r\n      </div>\r\n      <button class=\"submit-button\" type=\"submit\">Войти</button>\r\n    </form>\r\n    <p v-if=\"error\" class=\"error\">{{ error }}</p>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {useAuthStore} from \"@/auth\";\r\nimport PasswordComponent from \"@/components/start_page_components/passwordInput.vue\";\r\n\r\nexport default {\r\n  name: 'LoginComponent',\r\n  components: {PasswordComponent},\r\n  props: {\r\n    error: String,\r\n  },\r\n  data() {\r\n    return {\r\n      username: '',\r\n      password: '',\r\n    }\r\n  },\r\n  methods: {\r\n    async login() {\r\n      const authStore = useAuthStore();\r\n      if (!this.username || !this.password) {\r\n        this.$emit('setError', \"Не все поля заполнены.\" +\r\n            \" Пожалуйста, заполните пропущенные поля и попробуйте ещё раз.\");\r\n      } else {\r\n        return authStore.login({username: this.username, password: this.password});\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AA4BA,SAAQA,YAAY,QAAO,QAAQ;AACnC,OAAOC,iBAAgB,MAAO,sDAAsD;AAEpF,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,UAAU,EAAE;IAACF;EAAiB,CAAC;EAC/BG,KAAK,EAAE;IACLC,KAAK,EAAEC;EACT,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,KAAKA,CAAA,EAAG;MACZ,MAAMC,SAAQ,GAAIZ,YAAY,CAAC,CAAC;MAChC,IAAI,CAAC,IAAI,CAACQ,QAAO,IAAK,CAAC,IAAI,CAACC,QAAQ,EAAE;QACpC,IAAI,CAACI,KAAK,CAAC,UAAU,EAAE,wBAAuB,GAC1C,+DAA+D,CAAC;MACtE,OAAO;QACL,OAAOD,SAAS,CAACD,KAAK,CAAC;UAACH,QAAQ,EAAE,IAAI,CAACA,QAAQ;UAAEC,QAAQ,EAAE,IAAI,CAACA;QAAQ,CAAC,CAAC;MAC5E;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}